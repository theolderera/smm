# MongoDB configuration
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=your_secure_password
MONGO_URI=mongodb://admin:your_secure_password@mongodb:27017/smmplusai?authSource=admin

# Backend configuration
NODE_ENV=development
BACKEND_PORT=3000
JWT_SECRET=your_jwt_secret_key_here
SESSION_SECRET=your_session_secret_key_here

# AI APIs
GPT_API_KEY=your_openai_gpt4_key
MIDJOURNEY_API_KEY=your_midjourney_key
CLAUDE_API_KEY=your_anthropic_claude_key

# Social Media APIs
SOCIAL_API_KEYS={"facebook":"your_facebook_api_key","instagram":"your_instagram_api_key","tiktok":"your_tiktok_api_key","linkedin":"your_linkedin_api_key"}

# Payment Gateway (e.g., Stripe for subscription plans)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Canva Integration
CANVA_API_KEY=your_canva_api_key

# Security and Compliance
ENCRYPTION_KEY=your_32_char_encryption_key_here
GDPR_COMPLIANCE=true

# App-specific configurations
ALLOWED_ORIGINS=http://localhost:3000,https://www.smmplus.ai
SUPPORTED_LANGUAGES=to,ru,en,uz,fa